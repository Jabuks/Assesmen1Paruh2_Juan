<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Book List</title>
  <style>
    table, th, td {
      border: 1px solid black;
      border-collapse: collapse;
    }
    th, td {
      padding: 5px;
      text-align: left;
    }
  </style>
</head>
<body>
  <h1>Book List</h1>
  <table>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>Author</th>
      <th>Year Written</th>
      <th>Edition</th>
      <th>Price</th>
    </tr>
    <% books.forEach(function(book) { %>
      <tr>
        <td><%= book.id %></td>
        <td><%= book.title %></td>
        <td><%= book.author %></td>
        <td><%= book.year_written %></td>
        <td><%= book.edition %></td>
        <td>$<%= book.price %></td>
      </tr>
    <% }); %>
  </table>
</body>
</html>
